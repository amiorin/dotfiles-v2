# focus to desktop or cycle windows forward (bsp and stack) if already there
f1 : happy-yabai cmds move-or-next 1
f2 : happy-yabai cmds move-or-next 2
f3 : happy-yabai cmds move-or-next 3
f4 : happy-yabai cmds move-or-next 4
f5 : happy-yabai cmds move-or-next 5
f6 : happy-yabai cmds move-or-next 6
f7 : happy-yabai cmds move-or-next 7
f8 : happy-yabai cmds move-or-next 8
f9 : happy-yabai cmds move-or-next 9
f10 : happy-yabai cmds move-or-next 10

# move to desktop or cycle windows backward (bsp and stack) if already there
alt - f1 : happy-yabai cmds move-or-prev 1
alt - f2 : happy-yabai cmds move-or-prev 2
alt - f3 : happy-yabai cmds move-or-prev 3
alt - f4 : happy-yabai cmds move-or-prev 4
alt - f5 : happy-yabai cmds move-or-prev 5
alt - f6 : happy-yabai cmds move-or-prev 6
alt - f7 : happy-yabai cmds move-or-prev 7
alt - f8 : happy-yabai cmds move-or-prev 8
alt - f9 : happy-yabai cmds move-or-prev 9
alt - f10: happy-yabai cmds move-or-prev 10

# swap to desktop or cycle windows backward (bsp and stack) if already there
shift - f1  : happy-yabai cmds swap-or-prev 1
shift - f2  : happy-yabai cmds swap-or-prev 2
shift - f3  : happy-yabai cmds swap-or-prev 3
shift - f4  : happy-yabai cmds swap-or-prev 4
shift - f5  : happy-yabai cmds swap-or-prev 5
shift - f6  : happy-yabai cmds swap-or-prev 6
shift - f7  : happy-yabai cmds swap-or-prev 7
shift - f8  : happy-yabai cmds swap-or-prev 8
shift - f9  : happy-yabai cmds swap-or-prev 9
shift - f10 : happy-yabai cmds swap-or-prev 10

# cycle layout bsp and stack
f12 : happy-yabai cmds cycle-layout

# fix macos hide window
cmd - h : skhd -k "ctrl - b"; skhd -k "h"
