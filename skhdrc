# prev tab and next tab in chrome
cmd - i : skhd -k "shift + cmd - 0x21"
cmd - o : skhd -k "shift + cmd - 0x1E"
# move tab left and right in chrome with cmd - , and .
cmd - 0x2B : skhd -k "shift + ctrl - 0x74"
cmd - 0x2F : skhd -k "shift + ctrl - 0x79"
# alt + shift - 0x21 : skhd -k "alt - 7"
# alt + shift - 0x1E : skhd -k "alt - 8"
alt - 0x21 : skhd -k "alt - 5"
# alt - 0x1E : skhd -k "alt - 6"
