cmd + alt - a : happy-yabai cmds move-or-next 1
cmd + alt - z : happy-yabai cmds move-or-next 2
cmd + alt - s : happy-yabai cmds move-or-next 3
cmd + alt - d : happy-yabai cmds move-or-next 4
cmd + alt - f : happy-yabai cmds move-or-next 5
cmd + alt - x : happy-yabai cmds move-or-next 6
cmd + alt - c : happy-yabai cmds move-or-next 7
cmd + alt - v : happy-yabai cmds move-or-next 8
cmd + alt - g : happy-yabai cmds move-or-next 9
cmd + alt - b : happy-yabai cmds move-or-next 10

# move to desktop or cycle windows backward (bsp and stack) if already there
cmd + ctrl + alt - a : happy-yabai cmds move-or-prev 1
cmd + ctrl + alt - z : happy-yabai cmds move-or-prev 2
cmd + ctrl + alt - s : happy-yabai cmds move-or-prev 3
cmd + ctrl + alt - d : happy-yabai cmds move-or-prev 4
cmd + ctrl + alt - f : happy-yabai cmds move-or-prev 5
cmd + ctrl + alt - x : happy-yabai cmds move-or-prev 6
cmd + ctrl + alt - c : happy-yabai cmds move-or-prev 7
cmd + ctrl + alt - v : happy-yabai cmds move-or-prev 8
cmd + ctrl + alt - g : happy-yabai cmds move-or-prev 9
cmd + ctrl + alt - b : happy-yabai cmds move-or-prev 10

# swap to desktop or cycle windows backward (bsp and stack) if already there
cmd + shift + alt - a : happy-yabai cmds swap-or-prev 1
cmd + shift + alt - s : happy-yabai cmds swap-or-prev 2
cmd + shift + alt - d : happy-yabai cmds swap-or-prev 3
cmd + shift + alt - f : happy-yabai cmds swap-or-prev 4
cmd + shift + alt - g : happy-yabai cmds swap-or-prev 5
cmd + shift + alt - z : happy-yabai cmds swap-or-prev 6
cmd + shift + alt - x : happy-yabai cmds swap-or-prev 7
cmd + shift + alt - c : happy-yabai cmds swap-or-prev 8
cmd + shift + alt - v : happy-yabai cmds swap-or-prev 9
cmd + shift + alt - b : happy-yabai cmds swap-or-prev 10

# cycle layout bsp and stack
cmd + alt - u : happy-yabai cmds cycle-layout

# fix macos hide window
cmd - h : skhd -k "alt - h"
